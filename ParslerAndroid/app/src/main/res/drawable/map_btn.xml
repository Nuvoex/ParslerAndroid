<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	 <item android:state_pressed="true" >
		  <shape>
		  <solid
		  	   android:color="#A5A2A2" />
		  <stroke
			   android:width="1dp"
			   android:color="#ffffff" />
		  <corners
		  	 android:radius="3dp" />
		  <padding
			   android:left="5dp"
			   android:top="5dp"
			   android:right="5dp"
			   android:bottom="5dp" />
		  </shape>
	 </item>
	  <item>
		  <shape>
		  <gradient
			   android:startColor="#B9B5B5"
			   android:endColor="#B2ADAD"
			   android:angle="270" />
		  <stroke
			   android:width="1dp"
			   android:color="#ffffff" />
		  <corners
		   	   android:radius="3dp" />
		  <padding
			   android:left="5dp"
			   android:top="5dp"
			   android:right="5dp"
			   android:bottom="5dp" />
		 </shape>
	 </item>
</selector>